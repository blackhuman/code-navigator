{
  "name": "code-navigator",
  "displayName": "Code Navigator",
  "description": "Code Navigator",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "extensionDependencies": [
    "vscode.javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "configuration": [
      {
        "title": "Code Navigator",
        "properties": {
          "codenav.includes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "default": [],
            "description": "Only find files which include in the specified folders, if not specified, find in all files in the workspace.",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/glob": "^7.1.3",
    "@types/jscodeshift": "^0.11.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/strman": "^2.0.0",
    "@types/underscore": "^1.11.3",
    "@types/urijs": "^1.19.17",
    "@types/voca": "^1.4.1",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "mocha-explorer-launcher-scripts": "^0.3.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "async": "^3.2.1",
    "chai": "^4.3.4",
    "flexsearch": "^0.7.21",
    "jscodeshift": "^0.13.0",
    "recast": "^0.20.5",
    "strman": "^2.0.1",
    "underscore": "^1.13.1",
    "urijs": "^1.19.7",
    "voca": "^1.4.0"
  }
}
